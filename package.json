    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "4.11.0",
    "openapi3-ts": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.3.1",
    "rxjs": "^7.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
